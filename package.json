    "demo:cursor": "node cursor-integration.js",
    "demo:semantic": "node examples/semantic-demo.js",
    "test": "npm run demo && npm run demo:templates && npm run demo:semantic",
    "setup": "npm install && node -e \"console.log('âœ… Thinkerbell Pipeline ready!')\"",
    "create-templates": "node -e \"require('./examples/template-substitution').createExampleTemplateFiles()\"",
